{
  "name": "envalid",
  "version": "7.1.1",
  "description": "Validation for your environment variables",
  "main": "dist/index.js",
  "module": "dist/envalid.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=8.12"
  },
  "engineStrict": true,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdx build",
    "coverage": "tsdx test --coverage",
    "lint": "tsdx lint src tests",
    "prepare": "tsdx build",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/af/envalid"
  },
  "keywords": [
    ".env",
    "dotenv",
    "env",
    "env var",
    "environment",
    "environment variable",
    "validation"
  ],
  "devDependencies": {
    "husky": "4.3.6",
    "tsdx": "^0.14.1",
    "typescript": "^4.1.3"
  },
  "author": "Aaron Franks",
  "license": "MIT",
  "dependencies": {},
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix",
      "pre-push": "yarn test"
    }
  }
}
